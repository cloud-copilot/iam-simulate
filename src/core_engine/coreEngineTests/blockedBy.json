[
  {
    "name": "Blocked by SCP and RCP together - explicit denies",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::mybucket/testobject",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user",
      "context": {}
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "*"
          }
        ]
      }
    ],
    "serviceControlPolicies": [
      {
        "orgIdentifier": "ou-root",
        "policies": [
          {
            "Version": "2012-10-17",
            "Statement": [
              {
                "Effect": "Deny",
                "Action": "s3:GetObject",
                "Resource": "*"
              }
            ]
          }
        ]
      }
    ],
    "resourceControlPolicies": [
      {
        "orgIdentifier": "ou-root",
        "policies": [
          {
            "Version": "2012-10-17",
            "Statement": [
              {
                "Effect": "Deny",
                "Action": "s3:GetObject",
                "Resource": "*",
                "Principal": "*"
              }
            ]
          }
        ]
      }
    ],
    "expected": {
      "response": "ExplicitlyDenied",
      "blockedBy": ["scp", "rcp"]
    }
  },
