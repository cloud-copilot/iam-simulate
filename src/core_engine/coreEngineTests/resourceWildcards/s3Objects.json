[
  {
    "name": "Allow with Resource wildcard superset of request wildcard",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/photos/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "Implicit deny with Resource wildcard non-overlap",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/private/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/public/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "Allow with NotResource wildcard excluding private prefix",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/public/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "NotResource": "arn:aws:s3:::my-bucket/private/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "Implicit deny with NotResource wildcard matching request",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/private/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "NotResource": "arn:aws:s3:::my-bucket/private/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "Explicit deny with Deny Resource wildcard matching request",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/private/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*"
          },
          {
            "Effect": "Deny",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/private/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "ExplicitlyDenied"
    }
  },
  {
    "name": "Allow when Deny Resource wildcard does not match",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/public/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*"
          },
          {
            "Effect": "Deny",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/private/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "Allow when Deny Resource resource is a subset of request wildcard",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*"
          },
          {
            "Effect": "Deny",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/private/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "Explicit deny with Deny NotResource wildcard excluding public prefix",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/private/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*"
          },
          {
            "Effect": "Deny",
            "Action": "s3:GetObject",
            "NotResource": "arn:aws:s3:::my-bucket/public/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "ExplicitlyDenied"
    }
  },
  {
    "name": "Allow when Deny NotResource wildcard does not apply",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket/public/*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*"
          },
          {
            "Effect": "Deny",
            "Action": "s3:GetObject",
            "NotResource": "arn:aws:s3:::my-bucket/public/*"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  }
]
