  {
    "name": "Ignored conditions in session policy are reported",
    "request": {
      "action": "s3:GetObject",
      "resource": {
        "resource": "arn:aws:s3:::mybucket/testobject",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:sts::123456789012:assumed-role/test-role/session-name",
      "context": {}
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::mybucket/testobject"
          }
        ]
      }
    ],
    "sessionPolicy": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": "s3:*",
          "Resource": "*",
          "Condition": {
            "StringEquals": {
              "aws:SourceVpc": "vpc-123456"
            }
          }
        }
      ]
    },
    "simulation": {
      "mode": "Discovery"
    },
    "expected": {
      "response": "Allowed",
      "ignoredConditions": {
        "session": {
          "allow": [{ "key": "aws:SourceVpc", "op": "StringEquals", "values": ["vpc-123456"] }]
        }
      }
    }
  }
