  {
    "name": "Identity allows and denies different actions, resource policy allows denied action - same account",
    "request": {
      "action": "s3:DeleteObject",
      "resource": {
        "resource": "arn:aws:s3:::mybucket/testobject",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user",
      "context": {}
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "*"
          },
          {
            "Effect": "Deny",
            "Action": "s3:DeleteObject",
            "Resource": "*"
          }
        ]
      }
    ],
    "resourcePolicy": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": "s3:DeleteObject",
          "Resource": "arn:aws:s3:::mybucket/testobject",
          "Principal": {
            "AWS": "arn:aws:iam::123456789012:user/test-user"
          }
        }
      ]
    },
    "expected": {
      "response": "ExplicitlyDenied",
      "blockedBy": ["identity"]
    }
  }
