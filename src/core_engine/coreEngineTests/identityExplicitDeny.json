        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Deny",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::mybucket/*"
          }
        ]
      }
    ],
    "resourcePolicy": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": "s3:GetObject",
          "Resource": "arn:aws:s3:::mybucket/testobject",
          "Principal": {
            "AWS": "arn:aws:iam::123456789012:role/test-role"
