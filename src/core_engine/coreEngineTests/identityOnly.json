[
  {
    "name": "implicit deny when no policies",
    "request": {
      "action": "s3:ListAllMyBuckets",
      "resource": {
        "resource": "*",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user",
      "context": {}
    },
    "identityPolicies": [],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "implicit deny when no applicable policies",
    "request": {
      "action": "s3:ListAllMyBuckets",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }

    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "allow with identity matching policy",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "allow with a wildcard resource",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "*"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "Deny with explicit deny policy",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Deny",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "ExplicitlyDenied"
    }
  },
  {
    "name": "Deny with an allow and a deny policy",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          },
          {
            "Effect": "Deny",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "ExplicitlyDenied"
    }
  },
  {
    "name": "Implicit deny when allowed for a different resource",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::another-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "allow when conditions match",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user",
      "context": {
        "s3:prefix": "allowed-prefix/"
      }
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket",
            "Condition": {
              "StringEquals": {
                "s3:prefix": "allowed-prefix/"
              }
            }
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "Matches Not Action",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "NotAction": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "Multiple Not Actions Should deny actions in list",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "NotAction": ["s3:ListBucket", "s3:GetObject"],
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "should match a not resource",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "123456789012"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "NotResource": "arn:aws:s3:::a-different-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "Allowed"
    }
  },
  {
    "name": "cross account requests are implicitly denied",
    "request": {
      "action": "s3:ListBucket",
      "resource": {
        "resource": "arn:aws:s3:::my-bucket",
        "accountId": "987654321098"
      },
      "principal": "arn:aws:iam::123456789012:user/test-user"
    },
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket"
          }
        ]
      }
    ],
    "expected": {
      "response": "ImplicitlyDenied"
    }
  },
  {
    "name": "context key with variable",
    "identityPolicies": [
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::my-bucket",
            "Condition": {
              "StringEquals": {
                "s3:prefix": "allowed-prefix/"
              }
            }
          },
          {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::my-bucket/*",
            "Condition": {
              "StringNotEquals": {
                "s3:ExistingObjecttag/Classification": "proprietary"
              }
            }
          }
        ]
      }
    ],
    "serviceControlPolicies": [],
    "request": {
      "action": "s3:GetObject",
      "principal": "arn:aws:iam::123456789012:user/username",
      "resource": {
        "accountId": "123456789012",
        "resource": "arn:aws:s3:::my-bucket/abcd"
      },
      "context": {
        "s3:ExistingObjectTag/Classification": "proprietary"
      }
    },
    "expected": {
      "response": "ImplicitlyDenied"
    }
  }
]